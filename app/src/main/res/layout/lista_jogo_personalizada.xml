<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:baselineAligned="false">

        <TextView
            android:id="@+id/lista_jogos_personalizado_NomeBloco"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_marginStart="10dp"
            android:baselineAligned="false"
            app:layout_constraintBaseline_toBaselineOf="@+id/lista_jogos_personalizado_numeros"
            app:layout_constraintEnd_toStartOf="@+id/lista_jogos_personalizado_numeros"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/lista_jogos_personalizado_numeros"
            android:layout_width="0dp"
            android:layout_height="35dp"
            android:layout_marginEnd="10dp"
            android:baselineAligned="false"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/lista_jogos_personalizado_NomeBloco"
            app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>